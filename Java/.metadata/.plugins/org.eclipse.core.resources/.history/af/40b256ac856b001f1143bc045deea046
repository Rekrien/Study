package matriz;
import java.util.Scanner;
public class exer5 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int inv[][] = new int[3][4], qt[] = new int[4];
		for (int i=0; i<3; i++) {
			for (int j=0; j<4; j++) {
				System.out.print("Digite a quantidade do produto "+(i+1)+" no "+(j+1)+"ยบ armazem");
				inv[i][j]=Integer.parseInt(sc.next());
				qt[i]+=inv[i][j];
			}
		}
		for (int i=0; i<3; i++) {
			System.out.print((i+1)+"ยบ produto: "+qt[i]);
		}
		sc.close();
	}
}
